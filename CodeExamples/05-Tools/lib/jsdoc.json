{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"includePattern": ".+\\.js$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"recurse": true,
		"encoding": "utf8",
		"destination": "doc",
		"template": "node_modules/docdash"
	}
}